<div class="page-header">
  <h1>
    Measures of successes
    <a href="/measures_of_successes/new" class="btn btn-lg btn-success">New Measures of success</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @measures_of_successes.each do |measures_of_success| %>
      <tr>
        <td>
          <a href="/measures_of_successes/<%= measures_of_success.id %>" class="btn btn-primary">Show</a>
          <a href="/measures_of_successes/<%= measures_of_success.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_measures_of_success/<%= measures_of_success.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @measures_of_successes, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
